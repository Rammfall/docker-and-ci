{
  "name": "docker-and-ci",
  "version": "1.0.0",
  "description": "It is test repo for testing CI/deploy",
  "main": "index.js",
  "scripts": {
    "node-ts": "ts-node src/index.ts",
    "dev": "nodemon",
    "test": "jest",
    "lint:js": "eslint ./src/**/*.ts --format stylish --color",
    "lint:css": "stylelint ./**/*.scss --fix",
    "lint": "npm run lint:js && npm run lint:css",
    "postinstall": "tsc && node src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rammfall/docker-and-ci.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rammfall/docker-and-ci/issues"
  },
  "homepage": "https://github.com/Rammfall/docker-and-ci#readme",
  "dependencies": {
    "express": "^4.17.1",
    "sequelize": "^5.19.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.8",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^5.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged jest",
      "pre-push": "lint-staged jest"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "public"
  ],
  "lint-staged": {
    "*.ts": "eslint",
    "*.scss": "stylelint"
  }
}
